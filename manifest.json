{
  "name": "Remove Premium Placeholders",
  "version": "0.0.2",
  "description": "Remove Premium Placeholders from some news web sites",
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2,
  "browser_action": {
    "default_icons": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    },
    "default_popup": "popup.html"
  }
}
